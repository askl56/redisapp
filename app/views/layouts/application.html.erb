<!DOCTYPE html>
<html>
<head>
  <title>Redisapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= content_tag :div, notice, class: "text-success" if notice %>
  <%= content_tag :div, alert, class: "text-danger" if alert %>

  <div class="container">
    <%= yield %>
  </div>

</body>
</html>
